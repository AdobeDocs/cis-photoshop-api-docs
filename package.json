{
  "private": true,
  "name": "cis-photoshop-api-docs",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdobeDocs/dev-site-product-template"
  },
  "author": {
    "name": "Sudipta Khound",
    "url": "https://github.com/khound"
  },
  "dependencies": {
    "@adobe/gatsby-theme-aio": "4.9.2",
    "gatsby": "4.22.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "scripts": {
    "start": "gatsby build && gatsby serve",
    "start:prefix": "gatsby build --prefix-paths && gatsby serve --prefix-paths",
    "dev": "gatsby develop",
    "dev:https": "gatsby develop --https --host localhost.corp.adobe.com --port 9000",
    "build": "gatsby build",
    "build:incremental": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test:links": "remark src/pages --quiet --frail",
    "lint": "docker run --rm -e RUN_LOCAL=true --env-file '.github/super-linter.env' -v \"$PWD\":/tmp/lint github/super-linter:slim-v4.10.1"
  },
  "remarkConfig": {
    "plugins": [
      "remark-validate-links"
    ]
  },
  "packageManager": "yarn@3.2.1",
  "devDependencies": {
    "remark-cli": "^11.0.0",
    "remark-validate-links": "^12.1.0"
  }
}
